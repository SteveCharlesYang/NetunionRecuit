<div class="main">
    <div class="container">
        <form method="POST" id="signup-form" class="signup-form" action="#">
            <div>
                <h3>简介</h3>
                <fieldset>
                    {{>1}}
                </fieldset>
                <h3>个人信息</h3>
                <fieldset>
                    {{#if is_desktop}}
                        {{>2}}
                    {{else}}
                        {{>2_m}}
                    {{/if}}
                </fieldset>

                <h3>掌握/了解知识</h3>
                <fieldset>
                    {{>3}}
                </fieldset>

                <h3>校内/项目经历</h3>
                <fieldset>
                    {{>4}}
                </fieldset>

                <h3>爱好/评价/规划</h3>
                <fieldset>
                    {{>5}}
                </fieldset>
                <h3>提交</h3>
                <fieldset>
                    {{>6}}
                </fieldset>

            </div>
        </form>
    </div>
</div>

